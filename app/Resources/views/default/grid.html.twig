{% extends 'base.html.twig' %}

{% block body %}

	<div class="top">
		<div class="top-inner">
			{% image '@AppBundle/Resources/assets/img/logo-large.png' %}
			    <img src="{{ asset_url }}" alt="Example" />
			{% endimage %}
		</div>
	</div>
	
	<div class="grid">
		<div class="grid-inner">
		
		{% for r in 1..3 %}
			<div class="row row-{{ r }}">
		    {% for b in 1..5 %}
		    	<div class="block block-{{ r }}{{ b }}">
				</div>
			{% endfor %}
			</div>
		{% endfor %}

	</div>
	
{% endblock %}
